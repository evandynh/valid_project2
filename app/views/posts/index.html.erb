<h1>All Posts</h1>

<% @posts.each do |post| %>
      <div>
        <div><%= post.title %></div>
        <div><%= post.description %></div>
        <div><%= post.language %></div>
        <div><%= link_to 'Show', "posts/#{post.id}" %></div>
        <div><%#= link_to 'Edit', edit_user_post_path(post) %></div>
        <div><%#= link_to 'Delete', post, method: :delete, data: { confirm: 'Are you sure?' } %></div><br>
      </div>
    <% end %>

<%= link_to "Create new post", new_user_post_path %>
<br>
